       <H1 id="post">POST </H1>

       
       <%= form_tag("events/create", method:"post") do %>
       <div class = "container">
       <div class = "form-horizontal">
         <div class = "form-group">
           <label for = "ename" class = "col-sm-0">EVENT NAME</label>
           <div class = "col-sm-0">
			<%= text_field(:ename) %>
           </div>
         </div>
           
         <div class = "form-inline">
           <div class = "form-group">
             <label for ="date1" class = "col-sm-0">DATE</label>
             <div class="col-sm-0">
               <%= text_field(:date1) %>
               </div>
           </div>
             
             
           <div class = "form-group">
             <label for="time1" class="col-sm-0">TIME</label>
             <div class = "col-sm-0">
              <%= text_field(:time1) %>
             </div>
               
            <div class = "container">
            <div class = "form-horizontal">
				<div class = "form-group">
				<label for = "ename" class = "col-sm-0">EVENT DESCRIPTION</label>
					<div class = "col-sm-0">
					<%= text_field(:edes) %>
            </div>
            </div>
            
           
               
         </div>
       </div>
     </div>
        <div id="nav">     
        </div>

	</button type <%= submit_tag("Submit") %> </button>
  <% end %>         
